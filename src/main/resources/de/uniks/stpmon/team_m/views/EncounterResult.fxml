<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<VBox fx:id="resultVBox" maxHeight="250.0" maxWidth="550.0" minHeight="250.0" minWidth="550.0" prefHeight="250.0" prefWidth="550.0" styleClass="hBoxLightGreen" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uniks.stpmon.team_m.controller.subController.EncounterResultController">
   <HBox maxHeight="190.0" maxWidth="550.0" minHeight="190.0" minWidth="550.0" prefHeight="190.0" prefWidth="550.0" spacing="30.0">
      <VBox.margin>
         <Insets />
      </VBox.margin>
      <TextFlow maxHeight="200.0" maxWidth="550.0" minHeight="200.0" minWidth="550.0" prefHeight="200.0" prefWidth="550.0" textAlignment="CENTER">
         <Text fx:id="informationText" strokeType="OUTSIDE" strokeWidth="0.0">
            <font>
               <Font name="Comic Sans MS" size="17.0" />
            </font></Text>
      </TextFlow>
   </HBox>
   <HBox alignment="CENTER" maxHeight="60.0" maxWidth="550.0" minHeight="60.0" minWidth="550.0" prefHeight="60.0" prefWidth="550.0">
      <VBox.margin>
         <Insets />
      </VBox.margin>
      <Button fx:id="okButton" layoutX="10.0" layoutY="30.0" mnemonicParsing="false" onAction="#clickOK" prefHeight="20.0" prefWidth="120.0" styleClass="buttonsYellow" text="%OK">
         <HBox.margin>
            <Insets bottom="10.0" left="20.0" top="10.0" />
         </HBox.margin>
      </Button>
   </HBox>
</VBox>
